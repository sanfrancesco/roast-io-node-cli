{
  "name": "roast",
  "version": "2.2.2",
  "description": "CLI for deploying single page apps to Roast.io",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "./node_modules/semistandard/bin/cmd.js ./src/**/*",
    "build": "rm -rf dist && ./node_modules/.bin/babel src --no-comments --compact='true' --out-dir dist"
  },
  "bin": {
    "roast": "./dist/index.js"
  },
  "keywords": [
    "roast",
    "hosting",
    "cdn",
    "deploy",
    "single page app"
  ],
  "repository": {},
  "author": "Jonathan Otto <support@roast.io> (https://www.roast.io/)",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-babili": "^0.0.10",
    "babel-cli": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "flow-bin": "^0.38.0",
    "jasmine": "2.4.0",
    "nock": "^8.0.0",
    "semistandard": "^9.2.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "glob": "^7.1.1",
    "graceful-fs": "^4.1.11",
    "home-dir": "^1.0.0",
    "inquirer": "^3.0.1",
    "open": "^0.0.5",
    "semaphore": "^1.0.5",
    "update-notifier": "^1.0.3"
  }
}
